{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "jsx": "preserve",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "es2017"
    ],
    "baseUrl": ".",
    "outDir": "./dist",
    "moduleResolution": "node",
    "declaration": true,
    "strict": true,
    "allowJs": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "isolatedModules": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "paths": {
      "components/*": [
        "./components/*"
      ]
    }
  },
  "files": [
    "index.d.ts"
  ],
  "exclude": [
    "dist",
    ".next",
    "out",
    "next.config.js",
    "./node_modules"
  ],
  "include": [
    "index.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
